	<queryString><![CDATA[SELECT 
 i.DESCRIPTION AS DESCRIPTION, 
 w.Name  AS WAREHOUSE,
 p.Value AS PRODUCTCODE,
 il.QtyCount AS QTYCOUNT,
 P.NAME  AS PRODUCT,
 org.name as organisation,
 loc.value as locator,
 i.documentno
FROM 
    M_Inventory i, M_WAREHOUSE w, M_INVENTORYLINE il, AD_Org org, M_Locator loc,M_PRODUCT p
    	
WHERE 
    i.M_Inventory_ID IN (select M_Inventory_ID from M_INVENTORYLINE 
			where M_INVENTORYLINE_ID =$P{RECORD_ID})
AND i.M_WAREHOUSE_ID=w.M_Warehouse_ID
AND il.M_Inventory_ID=i.M_Inventory_ID
AND i.ad_org_id = org.ad_org_id
AND loc.m_locator_id=il.m_locator_id
AND il.M_Product_ID=p.M_Product_ID
ORDER BY il.m_locator_id,il.Line]]></queryString>

