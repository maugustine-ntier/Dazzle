	<queryString><![CDATA[SELECT 
 i.DESCRIPTION AS description, 
 w.Name  AS warehouse,
 p.Value AS productcode,
 P.NAME  AS product,
 il.QtyCount AS qtycount,
 il.QtyBook AS qtybook,
 org.name as organisation,
 i.documentno
FROM 
    M_Inventory i, M_WAREHOUSE w, AD_Org org, M_INVENTORYLINE il
    LEFT JOIN M_PRODUCT p ON il.M_Product_ID=p.M_Product_ID
WHERE 
    i.M_Inventory_ID=$P{RECORD_ID}
AND i.M_WAREHOUSE_ID=w.M_Warehouse_ID
AND il.M_Inventory_ID=i.M_Inventory_ID
AND i.ad_org_id = org.ad_org_id
AND il.QtyBook <> il.QtyCount
ORDER BY il.Line]]></queryString>

