	<queryString><![CDATA[select prod.M_PRODUCT_ID as ITEMNUMBER, prod.VALUE as PRODUCT, prod.DESCRIPTION as ITEMDESC,
i.DOCUMENTNO, i.DATEINVOICED, bp.VALUE as DEBTORID, 
bp.NAME as DEBTORNAME, uom.NAME as UNIT,inL.QTYINVOICED, inL.PRICEACTUAL as UNITPRICE, inL.LINENETAMT as EXTPRICE
from M_PRODUCT prod
LEFT OUTER JOIN  C_INVOICELINE inL ON prod.M_PRODUCT_ID = inL.M_PRODUCT_ID
LEFT OUTER JOIN  C_UOM uom ON inL.C_UOM_ID = uom.C_UOM_ID
LEFT OUTER JOIN  C_INVOICE i ON inL.C_INVOICE_ID = i.C_INVOICE_ID
LEFT OUTER JOIN  C_BPARTNER bp ON i.C_BPARTNER_ID = bp.C_BPARTNER_ID
where i.DATEINVOICED between $P{FROM} and $P{TO}
order by 1 desc, 4 desc]]></queryString>

