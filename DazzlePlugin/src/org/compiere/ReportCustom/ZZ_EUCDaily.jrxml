	<queryString>
		<![CDATA[SELECT euc.documentref, euc.apm_ref, euc.orderno, supp.name as supplier,
    CASE WHEN (euc.date_proc_at_dcac is null AND euc.date_permit_req_sub_to_dcac is null)
         THEN 'ACD'
    WHEN (euc.date_proc_at_dcac is null AND euc.date_permit_req_sub_to_dcac is not null)
         THEN 'DCAC'
    END as Location,
    coalesce(euc.date_permit_req_sub_to_dcac ,euc.date_proc_at_dcac) as DateAtLoc,
    (SELECT name FROM "ad_ref_list" WHERE ad_reference_id = 1000005 AND value = status ) AS status,
    ( SELECT name FROM "ad_ref_list" WHERE ad_reference_id = 1000004 AND value = request_urgency ) AS request_type,
    euc.followup_reason as comments
FROM zz_euc euc
LEFT JOIN c_bpartner apm ON apm.c_bpartner_id=euc.apm_id AND apm.isapm='Y'
LEFT JOIN c_bpartner supp ON supp.c_bpartner_id=euc.foreign_supplier_id AND supp.isforeignsupplier='Y'
WHERE euc.status='FO']]>
	</queryString>
