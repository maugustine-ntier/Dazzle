	<queryString><![CDATA[select
pr.value,pr.name as product,
mml.movementqty,mm.movementdate,
loc.value as locatorfrom,locto.value as locatorto,
adu.name as username
From m_movement mm
LEFT OUTER JOIN m_movementline mml ON mm.m_movement_ID=mml.m_movement_ID
LEFT OUTER JOIN m_product pr ON pr.m_product_id=mml.m_product_id
LEFT OUTER JOIN m_locator loc ON loc.m_locator_id=mml.m_locator_id
LEFT OUTER JOIN m_locator locto ON locto.m_locator_id=mml.m_locatorto_id
LEFT OUTER JOIN ad_user adu  ON adu.ad_user_id=mm.createdby
where mm.m_movement_id = $P{RECORD_ID}]]></queryString>

