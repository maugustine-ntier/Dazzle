	<queryString><![CDATA[Select DISTINCT prod.VALUE as ServiceRef, inL.QTYINVOICED as Qty,
prod.DESCRIPTION as Description, inL.LINENETAMT as Extprice,
inL.PRICEACTUAL as UnitPrice,
uom.NAME as Unit,
i.C_ORDER_ID as ORDERNO, i.DOCUMENTNO as TaxInvoice,
i.GRANDTOTAL as Total, i.DATEINVOICED as InvDate,pt.VALUE as PAYMENTTERM,
bp.NAME as BusPatna, bp.VALUE as DebtorID, bp.DUNS as GrossTonnage,
l.ADDRESS1 as BpAdd1, l.ADDRESS2 as BpAdd2, l.ADDRESS3 as
BpAdd3, l.ADDRESS4 as BpAdd4,
l2.ADDRESS1 as ADD1, l2.ADDRESS2 as ADD2, l2.ADDRESS3 as ADD3,
l2.ADDRESS4 as ADD4, dt.NAME as DOCTYPE 
from C_Invoice i
LEFT OUTER JOIN C_DOCTYPE dt ON i.C_DOCTYPETARGET_ID = dt.C_DOCTYPE_ID 
LEFT OUTER JOIN C_Invoiceline inL ON i.C_INVOICE_ID =inL.C_INVOICE_ID
LEFT OUTER JOIN C_PAymentTerm pt ON i.C_PAYMENTTERM_ID = pt.C_PAYMENTTERM_ID
LEFT OUTER JOIN C_UOM uom ON inL.C_UOM_ID = uom.C_UOM_ID
LEFT OUTER JOIN M_PRODUCT prod ON inL.M_PRODUCT_ID = prod.M_PRODUCT_ID
LEFT OUTER JOIN C_BPARTNER bp ON i.C_BPARTNER_ID = bp.C_BPARTNER_ID
LEFT OUTER JOIN C_LOCATION l ON i.C_BPARTNER_LOCATION_ID = l.C_LOCATION_ID
LEFT OUTER JOIN C_LOCATION l2 ON i.AD_ORG_ID = l2.AD_ORG_ID
where i.C_INVOICE_ID = 1000000
and (l2.C_LOCATION_ID in (select min(C_LOCATION_ID) from
 C_LOCATION where AD_ORG_ID = l2.AD_ORG_ID) or l2.C_LOCATION_ID is null)]]></queryString>

