	<queryString><![CDATA[SELECT  bp.NAME as PARTNERNAME, bp.VALUE as CUSTNO,
        l.ADDRESS1 as ADD1, l.ADDRESS2 as ADD2, l.ADDRESS3 as ADD3, l.ADDRESS4 as ADD4, l.POSTAL as POSTCODE,l.CITY,
        o.DOCUMENTNO DOCUMENTNO, o.DATEPROMISED, o.GRANDTOTAL, O.C_ORDER_ID as ORDERNO,
        dt.PRINTNAME as DOCTYPEDESC,dt.NAME as DOCTYPE ,
        prod.DESCRIPTION as LINEDESC, 
        uom.NAME as UOM,
        u.NAME as SALESREP,
        ol.QTYORDERED as QTY, ol.PRICEACTUAL as UNITPRICE, ol.LINENETAMT
FROM C_ORDER o , C_ORDERLINE ol, C_DOCTYPE dt,  
        C_BPARTNER bp, C_BPARTNER_LOCATION bpl, 
        C_LOCATION l, M_PRODUCT prod, C_UOM  uom, AD_USER u
WHERE  $P{RECORD_ID} = o.C_ORDER_ID(+)  
    and o.C_ORDER_ID = ol.C_ORDER_ID(+)
    and o.C_DOCTYPETARGET_ID  = dt.C_DOCTYPE_ID(+)
    and o.BILL_BPARTNER_ID = bp.C_BPARTNER_ID
    and o.BILL_LOCATION_ID = bpl.C_BPARTNER_LOCATION_ID(+)
    and o.SALESREP_ID = u.AD_USER_ID(+)
    and bpl.C_LOCATION_ID = l.C_LOCATION_ID(+)
    and ol.M_PRODUCT_ID = prod.M_PRODUCT_ID(+)
    AND PROD.C_UOM_ID = uom.C_UOM_ID(+)
ORDER BY o.C_ORDER_ID]]></queryString>

