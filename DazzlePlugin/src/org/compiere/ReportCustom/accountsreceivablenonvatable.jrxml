	<queryString><![CDATA[select c.documentno,c.dateinvoiced,bp.name as BPartner,c.grandtotal,ct.taxamt,tp.name as Transactiontype
from c_invoicetax ct,c_invoice c,c_doctype tp,c_bpartner bp,c_tax tax
where ct.c_invoice_id=c.c_invoice_id
and ct.c_tax_id=tax.C_tax_id
--and c.issotrx='Y'
and c.dateinvoiced >= $P{DateAcctFrom} 
and c.dateinvoiced <= $P{DateAcctTo}
and c.ad_client_id=$P{AD_Client_ID}
and tp.NAME IN ('AR Invoice','AP CreditMemo','AR Invoice Indirect')
--and tp.docbasetype in ('ARI','ARR','ARC','APP','API','APC')
and tp.c_doctype_id=c.c_doctype_id
and bp.c_bpartner_id=c.c_bpartner_id
and c.posted = 'Y'
and c.docstatus in ('CO','CL')
and c.reversal_id is null
--and c.docstatus !='RE'
and tax.rate = 0
order by c.dateinvoiced]]></queryString>

